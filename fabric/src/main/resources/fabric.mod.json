{
  "schemaVersion": 1,
  "id": "${base_name}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${author0}"
  ],
  "contact": {
    "homepage": "${homepageURL}",
    "sources": "${sourceCodeURL}",
    "issues": "${issueTrackerURL}"
  },
  "license": "${licence}",
  "icon": "icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.xolt.freecam.fabric.FreecamFabric"
    ],
    "modmenu": [
      "net.xolt.freecam.fabric.ModMenuIntegration"
    ]
  },
  "mixins": [
    "freecam-fabric.mixins.json",
    "freecam-common.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabric_loader_req}",
    "fabric": "*",
    "minecraft": "${fabric_mc_req}"
  }
}
