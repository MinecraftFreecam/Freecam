{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    ${modAuthorsJson}
  ],
  "contact": {
    "homepage": "${modHomepage}",
    "sources": "${modSource}",
    "issues": "${modIssues}"
  },
  "license": "${modLicense}",
  "icon": "icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.xolt.freecam.fabric.FreecamFabric"
    ],
    "modmenu": [
      "net.xolt.freecam.fabric.ModMenuIntegration"
    ]
  },
  "mixins": [
    "freecam-fabric.mixins.json",
    "freecam-common.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabricLoaderReq}",
    "fabric": "*",
    "minecraft": "${fabricMcReq}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.crowdin": "${modCrowdin}",
        "modmenu.curseforge": "${modCurseforge}",
        "modmenu.modrinth": "${modModrinth}",
        "modmenu.github_releases": "${modGhReleases}"
      }
    }
  }
}
